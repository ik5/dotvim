snippet str "String" b
"${1:key}": "${0:value}",
endsnippet

snippet num "Number" b
"${1:key}": ${0:value},
endsnippet

snippet arr "Array" b
[
	${VISUAL}$0
],
endsnippet

snippet nam "Named array" b
"${1:key}": [
	${VISUAL}$0
],
endsnippet

snippet obj "Object" b
{
	${VISUAL}$0
},
endsnippet

snippet namobj "Named object" b
"${1:key}": {
	${VISUAL}$0
},
endsnippet

snippet null "Null" b
"${0:key}": null,
endsnippet
